public class remove_duplicate_char {
    public static void main(String[] args) {
        String s="todayiwanttogotocollege";
        StringBuffer sb=new StringBuffer();
        int n=s.length();
        for(int i=0;i<n;i++){
            char ch=s.charAt(i);
            if(s.indexOf(ch)==i){
                sb.append(ch);
                
            }
        }
        System.out.println(sb);       
    }
    
}
